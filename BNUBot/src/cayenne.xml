<?xml version="1.0" encoding="utf-8"?>
<domains project-version="3.0">
<domain name="BNUBotDomain">
	<map name="BNUBotMap" location="BNUBotMap.map.xml"/>

	<node name="BNUBotDataNode"
		 datasource="BNUBotDomainNode.driver.xml"
		 factory="net.bnubot.db.conf.CayenneConfiguration"
		 schema-update-strategy="org.apache.cayenne.access.dbsync.ThrowOnPartialSchemaStrategy">
			<map-ref name="BNUBotMap"/>
	 </node>
</domain>
</domains>
