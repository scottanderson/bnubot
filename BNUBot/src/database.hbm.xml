<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping auto-import="false" package="bnubot.bot.database.pojo">
	<class name="Root" table="root">
  	  	<id column="id" name="id" type="java.lang.Long">
  	  	  	<generator class="increment"/>
  	  	</id>
  	  	<set name="accounts" access="field" inverse="true">
	        <key column="id"/>
	        <one-to-many class="Account"/>
	    </set>
	</class>
  	<class name="Account" table="account">
  	  	<id column="id" name="id" type="java.lang.Long">
  	  	  	<generator class="increment"/>
  	  	</id>
  	  	<many-to-one
			column="root"
			name="root"
			not-null="true"
			class="bnubot.bot.database.pojo.Root"
			/>
		<property
			column="access"
			name="access"
			type="java.lang.Long"/>
		<property
			column="name"
			name="name"
			not-null="true"
			type="java.lang.String"/>
		<property
			column="created"
			name="created"
			not-null="true"
			type="java.lang.String"/>
  	  	<set name="users" access="field" inverse="true">
	        <key column="id"/>
	        <one-to-many class="User"/>
	    </set>
  	</class>
  	<class name="User" table="user">
  	  	<id column="id" name="id" type="java.lang.Long">
  	  	  	<generator class="increment"/>
  	  	</id>
  	  	
		<property column="login" name="login" not-null="true" type="java.lang.String"/>
		<many-to-one
			column="account"
			name="account"
			not-null="true"
			class="bnubot.bot.database.pojo.Account"
			/>
		<property column="created" name="created" not-null="true" type="java.lang.String"/>
		<property column="lastSeen" name="lastSeen" not-null="true" type="java.lang.String"/>
		
  	</class>
</hibernate-mapping> 